--//Modules

local modules = {}

--// Scripts

-- Button
task.spawn(function()
	local script = button

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local Button = script.Parent
	local TweenService = game:GetService("TweenService")
	local Open = false
	local RealismSlide = script.Parent.Parent.Parent.GearUI
	local Audio = script.Parent.Sounds
	
	Button.MouseEnter:Connect(function()
		local Info = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
		local Tween = TweenService:Create(Button, Info, {Size = UDim2.new(0.45, 0, 0.25, 0)})
		
		Tween:Play()
		Audio.Hover:Play()
	end)
	
	Button.MouseLeave:Connect(function()
		local Info = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
		local Tween = TweenService:Create(Button, Info, {Size = UDim2.new(0.415, 0, 0.23, 0)})
		
		Tween:Play()
		Audio.Hover:Play()
	end)
	
	Button.Activated:Connect(function()
		Audio.Click:Play()
		local Info = TweenInfo.new(0.15, Enum.EasingStyle.Circular, Enum.EasingDirection.In)
		
		local Tween1 = TweenService:Create(Button, Info, {Size = UDim2.new(0.355, 0, 0.2, 0)})
		Tween1:Play()
		
		Tween1.Completed:Connect(function()
			local Tween2 = TweenService:Create(Button, Info, {Size = UDim2.new(0.415, 0, 0.23, 0)})
			Tween2:Play()
		end)
		
		if Open == false then
			local Info = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
			local Tween = TweenService:Create(RealismSlide, Info, {Position = UDim2.new(1, 0, 0.5, 0)})
			Tween:Play()
			Open = true
		else
			local Info = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
			local Tween = TweenService:Create(RealismSlide, Info, {Position = UDim2.new(1.4, 0, 0.5, 0)})
			Tween:Play()
			Open = false
		end
	end)
end)

-- Button
task.spawn(function()
	local script = button_2

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	local Button = script.Parent
	local TweenService = game:GetService("TweenService")
	local Open = false
	local GearSlide = script.Parent.Parent.Parent.GearUI
	local Audio = script.Parent.Sounds
	
	Button.MouseEnter:Connect(function()
		local Info = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
		local Tween = TweenService:Create(Button, Info, {Size = UDim2.new(0.45, 0, 0.25, 0)})
		
		Tween:Play()
		Audio.Hover:Play()
	end)
	
	Button.MouseLeave:Connect(function()
		local Info = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
		local Tween = TweenService:Create(Button, Info, {Size = UDim2.new(0.415, 0, 0.23, 0)})
		
		Tween:Play()
		Audio.Hover:Play()
	end)
	
	Button.Activated:Connect(function()
		Audio.Click:Play()
		local Info = TweenInfo.new(0.15, Enum.EasingStyle.Circular, Enum.EasingDirection.In)
		
		local Tween1 = TweenService:Create(Button, Info, {Size = UDim2.new(0.355, 0, 0.2, 0)})
		Tween1:Play()
		
		Tween1.Completed:Connect(function()
			local Tween2 = TweenService:Create(Button, Info, {Size = UDim2.new(0.415, 0, 0.23, 0)})
			Tween2:Play()
		end)
		
		if Open == false then
			local Info = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
			local Tween = TweenService:Create(GearSlide, Info, {Position = UDim2.new(1, 0, 0.5, 0)})
			Tween:Play()
			Open = true
		else
			local Info = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
			local Tween = TweenService:Create(GearSlide, Info, {Position = UDim2.new(1.4, 0, 0.5, 0)})
			Tween:Play()
			Open = false
		end
	end)
end)

-- Loading
task.spawn(function()
	local script = loading_2
	script.Disabled = true

	script:GetPropertyChangedSignal("Disabled"):Wait()

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	
	local prog = 0
	
	while wait() do
		prog = prog + 0.125
		script.Parent.Size = UDim2.new(prog/100, 0, 0.1, 0)
		script.Parent.Parent.LoadingLabel.Text = ("Loading... " ..math.floor(prog*2).. "%")
		if prog == 50 then
			script.Parent.Parent.LoadingProgress.Text = ("Loaded Vexecutor" ..script.Parent.Parent.Parent.VersionOfVexecutor.Value.. " Successfully")
			wait(1)
			script.Parent.Parent:Destroy()
		end
	end
	
	
end)

-- Load
task.spawn(function()
	local script = load

	local oldreq = require
	local function require(target)
		if modules[target] then
			return modules[target]()
		end
		return oldreq(target)
	end

	task.wait(7.62)
	local LoadFrame = script.Parent.Loading
	script.Parent.Enabled = true
	
	local Bar = LoadFrame.Loading1.Loading2
	
	local TweenService = game:GetService("TweenService")
	local BarTweenInfo = TweenInfo.new(math.random(5, 6), Enum.EasingStyle.Quint, Enum.EasingDirection.In)
	local Tween = TweenService:Create(Bar, BarTweenInfo, {Size = UDim2.new(1, 0, 1, 0)})
	Bar.Visible = true
	Tween:Play()
	
	Tween.Completed:Connect(function()
		LoadFrame.LoadingLabel.Text = "Loaded"
		task.wait(1.2)
		local NewInfo = TweenInfo.new(2, Enum.EasingStyle.Linear, Enum.EasingDirection.In)
		local Object1 = script.Parent.Loading.Vignette
		local Tween1 = TweenService:Create(Object1, NewInfo, {ImageTransparency = 1})
		Tween1:Play()
		
		local Object2 = script.Parent.Loading.Background
		local Tween2 = TweenService:Create(Object2, NewInfo, {ImageTransparency = 1})
		Tween2:Play()
		
		local Object3 = script.Parent.Loading.LoadingLabel
		local Tween3 = TweenService:Create(Object3, NewInfo, {TextTransparency = 1})
		Tween3:Play()
		
		local Object4 = script.Parent.Loading.UIStroke
		local Tween4 = TweenService:Create(Object4, NewInfo, {Transparency = 1})
		Tween4:Play()
		
		script.Parent.Loading.Loading1:Destroy()
		
		Tween4.Completed:Connect(function()
			script.Parent.MainGui.Visible = true
			LoadFrame:Destroy()
			local Info2 = TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
			local Tween5 = TweenService:Create(script.Parent.MainGui, Info2, {Position = UDim2.new(0.027, 0, 0.7, 0)})
			Tween5:Play()
		end)
	end)
end)
